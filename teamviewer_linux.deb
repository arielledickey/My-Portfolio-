	function changeLanguage(loc) {
		var pString = "/press/";
		var pIndex = document.URL.indexOf(pString);
		if (pIndex > -1)
			window.location = window.location.href.replace( /\/en\//i , '/' + loc.options[loc.selectedIndex].value + '/').substring(0, (pIndex + pString.length)) + "index.aspx";
		else 
			window.location = window.location.href.replace( /\/en\//i , '/' + loc.options[loc.selectedIndex].value + '/');
		expireDate = new Date();
		expireDate.setTime(expireDate.getTime() + (86400000)); // 1 Tag
		document.cookie = "language=" + loc.options[loc.selectedIndex].value + "; expires=" + expireDate.toUTCString() + "; path=/";
	}
</script>

